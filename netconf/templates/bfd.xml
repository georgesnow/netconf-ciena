<config
  xmlns:ncx="http://netconfcentral.org/ns/yuma-ncx"
  ncx:last-modified="2023-01-11T02:08:00Z" ncx:etag="12"
  xmlns="urn:ietf:params:xml:ns:netconf:base:1.0">
    <bfd xmlns="http://ciena.com/ns/yang/ciena-bfd">
    <profiles>
      <ip-bfd-profile>
        <name>BFD-PROFILE</name>
        <desired-min-tx-interval>10000</desired-min-tx-interval>
        <required-min-rx-interval>10000</required-min-rx-interval>
      </ip-bfd-profile>
      <ip-bfd-profile>
        <name>bfd_10s</name>
        <desired-min-tx-interval>10000000</desired-min-tx-interval>
        <required-min-rx-interval>10000000</required-min-rx-interval>
        <accelerate>false</accelerate>
      </ip-bfd-profile>
      <ip-bfd-profile>
        <name>bfd_1s</name>
        <desired-min-tx-interval>1000000</desired-min-tx-interval>
        <required-min-rx-interval>1000000</required-min-rx-interval>
        <accelerate>false</accelerate>
      </ip-bfd-profile>
      <ip-bfd-profile>
        <name>bfd_100ms</name>
        <accelerate>false</accelerate>
      </ip-bfd-profile>
      <ip-bfd-profile>
        <name>bfd_50ms</name>
        <desired-min-tx-interval>50000</desired-min-tx-interval>
        <required-min-rx-interval>50000</required-min-rx-interval>
        <accelerate>false</accelerate>
      </ip-bfd-profile>
      <ip-bfd-profile>
        <name>bfd_fast_10s</name>
        <desired-min-tx-interval>10000000</desired-min-tx-interval>
        <required-min-rx-interval>10000000</required-min-rx-interval>
      </ip-bfd-profile>
      <ip-bfd-profile>
        <name>bfd_fast_1s</name>
        <desired-min-tx-interval>1000000</desired-min-tx-interval>
        <required-min-rx-interval>1000000</required-min-rx-interval>
      </ip-bfd-profile>
      <ip-bfd-profile>
        <name>bfd_fast_100ms</name>
      </ip-bfd-profile>
      <ip-bfd-profile>
        <name>bfd_fast_50ms</name>
        <desired-min-tx-interval>50000</desired-min-tx-interval>
        <required-min-rx-interval>50000</required-min-rx-interval>
      </ip-bfd-profile>
      <ip-bfd-profile>
        <name>bfd_fast_10ms</name>
        <local-multiplier>3</local-multiplier>
        <desired-min-tx-interval>10000</desired-min-tx-interval>
        <required-min-rx-interval>10000</required-min-rx-interval>
        <accelerate>true</accelerate>
      </ip-bfd-profile>
      <ip-bfd-profile>
        <name>bfd_fast_3.3ms</name>
        <desired-min-tx-interval>3300</desired-min-tx-interval>
        <required-min-rx-interval>3300</required-min-rx-interval>
      </ip-bfd-profile>
    </profiles>
    <session-config-entry>
      <ip>
        <single-hop>
          <ipv4>
            <interface>{{ UPSTREAM_DEV }}-{{ DEV_TYPE }}-INT</interface>
            <profile>bfd_fast_10ms</profile>
          </ipv4>
        </single-hop>
      </ip>
    </session-config-entry>
  </bfd>
  </config>